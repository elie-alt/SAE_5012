<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}


        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {# <a href="{{path('admin')}}" >Dashboard</a>
		<a href="{{path('app_login')}}">Se connecter</a>
		<a href="{{path('app_register')}}">S'inscrire</a> #}

        <div {{ react_component('Navbar', { 'LinkBrand': path('admin'), 'LinkSignIn': path('app_login'), 'LinkSignUp':path('app_register') }) }}></div>


{# , 'LinkArticleShow': path('article_show') Dans le controller faire en sorte d'avoir une vue avec tous les articles #}
        {% block body %}
            <div id="article-list">
                {% include 'article/list.html.twig' with { articles } %}
            </div>
			<div>
				{% include 'widget/categories.html.twig' %}
			</div>
        {% endblock %}
    </body>
</html>
